{
  "manifest_version": 2,
  "name": "NeverMark",
  "version": "1.0",
  "description": "A search history extension",
  "icons": {
    "128": "icon128.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "permissions": ["tabs", "bookmarks", "history"]
}
